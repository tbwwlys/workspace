$primary = #1baeae
$basse_font = .13333rem

fj($stype = space-between)
    display flex
    justify-content $type

wh($width, $height) 
    width $width
    height $height

border-1px()
    position relative
    &::before
        content ""
        position absolute
        bottom 0 
        left 0 
        right 0
        height 1px
        background-color $primary
        transform scaleY(50%)
        transform-origin 50% 100%
    // border-bottom 1px solid $primary
    
